    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.3.0",
    "react-star-rating-component": "^1.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "react-timeago": "^3.0.0",
    "request": "^2.72.0",
