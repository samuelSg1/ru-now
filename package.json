{
  "name": "ru-now",
  "version": "0.1.0",
  "description": "An awesome app for hungry university people",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server",
    "api": "cross-env NODE_ENV=development nodemon ./server --exec babel-node --presets es2015,stage-0",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "prod": "cross-env NODE_ENV=production node server",
    "web": "node server",
    "s3": "npm run build && s3-cli put -P ./public/dist/bundle.js s3://runow/bundle.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ufv-js/ru-now.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "UFV-JS <luandro@gmail.com> (http://github.com/ufv-js)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ufv-js/ru-now/issues"
  },
  "homepage": "https://github.com/ufv-js/ru-now",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.8.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "ip": "^1.1.2",
    "offline-plugin": "^2.2.0",
    "react-hot-loader": "^3.0.0-alpha.12",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cheerio": "^0.20.0",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "cross-env": "^1.0.7",
    "express": "^4.13.4",
    "fb": "^1.0.2",
